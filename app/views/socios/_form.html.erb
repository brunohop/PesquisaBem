<%= simple_form_for(@socio) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :cpf, label:'CPF' ,:input_html => {:maxlength => 11, :size => 11} %>
    <%= f.input :dataNascimento, label:'Data de nascimento' %>
    <%= f.input :administrador %>
  </div>
  <%if @socio.empresa %>
  <%= f.hidden_field :empresa_id, value: @socio.empresa.id %>
  <% end %>
  <div class="form-actions">
  <%= f.button :submit, 'Salvar' %>
  </div>
<% end %>
